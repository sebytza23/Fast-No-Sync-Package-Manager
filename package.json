{
  "name": "fnspm",
  "module": "index.ts",
  "type": "commonjs",
  "version": "0.1.7",
  "devDependencies": {
    "@types/bun": "latest",
    "npmignore": "^0.3.1",
    "tsc-alias": "^1.8.10"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "bin": {
    "fnspm": "dist/index.js"
  },
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "prepack": "npmignore --auto"
  },
  "dependencies": {
    "enquirer": "^2.4.1"
  },
  "_moduleAliases": {
    "@": "src"
  },
  "files": [
    "dist/src/utils/types.d.ts",
    "dist/**/*.js"
  ],
  "permissions": "755",
  "license": "GPL-3.0",
  "private": false,
  "author": "Marin-Eusebiu È˜erban",
  "description": "A unified command-line interface for managing packages across multiple package managers (npm, yarn, pnpm, bun, and deno) with macOS optimization for iCloud sync and automatic package manager detection.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sebytza23/Fast-No-Sync-Package-Manager.git"
  },
  "main": "dist/index.js",
  "keywords": [
    "package-manager",
    "npm",
    "yarn",
    "pnpm",
    "bun",
    "deno",
    "nosync"
  ],
  "types": "./dist/src/utils/types.d.ts",
  "publishConfig": {
    "ignore": [
      "!dist/",
      "!dist/src/utils/types.d.ts",
      "src/",
      "test/",
      "*.ts"
    ]
  }
}